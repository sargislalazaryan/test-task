{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "prefer-const": "error",
    "indent": ["warn", 4],
    "max-len": ["error", {"code":  120}],
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["off"]
  }
}